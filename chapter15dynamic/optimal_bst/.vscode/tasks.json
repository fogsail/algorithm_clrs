{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "g++",
    "isShellCommand": true,
    "args": ["-g","optimal_bst.cpp","-o","optimal_bst"],
    "showOutput": "always",
    "problemMatcher":{
        "owner": "c++",
        "fileLocation": ["relative","${workspaceRoot}"],
        "pattern":{
            "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
        }
    }
}